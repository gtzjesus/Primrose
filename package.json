{
  "name": "primrose",
  "version": "1.0.0",
  "description": "e-commerce web application with node, exress, and mangoDB",
  "targets": {
    "main": false
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start:prod": "NODE_ENV=production & nodemon server.js",
    "debug": "ndb server.js",
    "watch:js": "parcel watch ./public/js/index.js dist-dir ./public/js dist-file bundle.js",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js --public-url ."
  },
  "author": "Jesus Gutierrez",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.1.5",
    "hpp": "^0.2.3",
    "html-to-text": "^6.0.0",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "4.1",
    "mongoose": "^5.13.19",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-sass": "^8.0.0",
    "nodemailer": "^6.9.1",
    "nodemon": "^2.0.22",
    "parcel": "^2.8.3",
    "pug": "^3.0.2",
    "sass": "^1.62.1",
    "sharp": "^0.32.1",
    "slugify": "^1.6.6",
    "stripe": "^7.0.0",
    "validator": "^13.9.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "buffer": "^5.7.1",
    "ndb": "^1.1.5",
    "parcel-bundler": "^1.12.3"
  },
  "engines": {
    "node": "18.16.0",
    "npm": "9.5.1"
  }
}
